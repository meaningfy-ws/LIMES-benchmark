	<SOURCE>
		<ID>src</ID>
		<ENDPOINT>http://localhost:7288/repositories/KIO_2_input</ENDPOINT>
		<VAR>?x</VAR>
		<PAGESIZE>2000</PAGESIZE>
        <RESTRICTION>?x a org:Organization</RESTRICTION>
        <PROPERTY>epo:hasLegalName AS nolang->lowercase RENAME legal_name</PROPERTY>
        <TYPE>SPARQL</TYPE>
	</SOURCE>
	<TARGET>
		<ID>tgt</ID>
		<ENDPOINT>http://localhost:7288/repositories/KIO_2_input</ENDPOINT>
		<VAR>?y</VAR>
		<PAGESIZE>2000</PAGESIZE>
        <RESTRICTION>?y a org:Organization</RESTRICTION>
        <PROPERTY>epo:hasLegalName AS nolang->lowercase RENAME legal_name</PROPERTY>
        <TYPE>SPARQL</TYPE>
	</TARGET>
	<METRIC>ExactMatch(x.legal_name, y.legal_name)</METRIC>
	<ACCEPTANCE>
		<THRESHOLD>0.95</THRESHOLD>
		<FILE>output/acceptance.ttl</FILE>
		<RELATION>owl:sameAs</RELATION>
	</ACCEPTANCE>
	<REVIEW>
		<THRESHOLD>0.7</THRESHOLD>
		<FILE>output/review.ttl</FILE>
		<RELATION>owl:sameAs</RELATION>
	</REVIEW>
	<EXECUTION>
		<REWRITER>default</REWRITER>
		<PLANNER>default</PLANNER>
		<ENGINE>default</ENGINE>
	</EXECUTION>
	<OUTPUT>NT</OUTPUT>
